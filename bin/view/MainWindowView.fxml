        </columns>
      </TableView>
      <Button fx:id="loadButton" layoutX="22.0" layoutY="30.0" mnemonicParsing="false" onAction="#loadData" text="wczytaj" />
      <Button fx:id="addButton" layoutX="28.0" layoutY="116.0" mnemonicParsing="false" onAction="#addData" text="dodaj" />
      <Label fx:id="nameLabel" layoutX="393.0" layoutY="25.0" text="Imie" />
      <Label fx:id="surnameLabel" layoutX="392.0" layoutY="82.0" text="Nazwisko" />
      <Label fx:id="roomLabel" layoutX="393.0" layoutY="141.0" text="nr pokoju" />
