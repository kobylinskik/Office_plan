      <TextField fx:id="roomField" layoutX="363.0" layoutY="157.0" />
      <TextField fx:id="startField" layoutX="567.0" layoutY="43.0" />
      <TextField fx:id="finishField" layoutX="567.0" layoutY="103.0" />
      <Button fx:id="saveButton" layoutX="25.0" layoutY="77.0" mnemonicParsing="false" onAction="#saveData" text="zapisz" />
      <Button fx:id="reportButton" layoutX="24.0" layoutY="157.0" mnemonicParsing="false" onAction="#createReport" text="raport" />
      <Canvas fx:id="officePlan" height="294.0" layoutX="374.0" layoutY="192.0" width="365.0" />
   </children>
</AnchorPane>
